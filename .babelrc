{
  "env": {
    "development": {
      "presets": [["env", { "modules": false }], "react"],
      "plugins": [
        
        ["transform-object-rest-spread", { "useBuiltIns": true }],
        "babel-plugin-transform-class-properties"
      ]
    },
    "test": {
      "presets": ["env", "react"],
      "plugins": [
        ["transform-object-rest-spread", { "useBuiltIns": true }],
        "babel-plugin-transform-class-properties"
      ]
    }
  }
}
